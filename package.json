{
	"name": "sweet-react-generator",
	"displayName": "vscode-extension-sandbox",
	"description": "Utility to help scaffold React code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.sweet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"commands": [
		{
			"command": "extension.sweet",
			"title": "New sweet Component"
		}
	],
	"menus": {
		"explorer/context": [
			{
				"command": "extension.sweet",
				"group": "navigation"
			}
		]
	}

}
