{
  "name": "sweet-react-generator",
  "displayName": "vscode-extension-sandbox",
  "description": "Opinionated utility to help scaffold React and related ecosystem code",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createDucks"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createDucks",
        "title": "Create Ducks Folder"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createDucks",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.14.10",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "@types/fs-extra": "^7.0.0",
    "fs-extra": "^8.0.1"
  }
}
